{
  "name": "small-talk",
  "version": "1.0.0",
  "bin": {
    "small-talk": "bin/small-talk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -a SmallTalkStack",
    "destroy": "npm run cdk destroy -a SmallTalkStack",
    "synth": "npm run cdk synth > template.yaml"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.4.0",
    "@types/node": "18.17.0",
    "aws-cdk": "2.88.0",
    "esbuild": "^0.18.16",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.88.0",
    "cheerio": "^1.0.0-rc.12",
    "constructs": "^10.2.40",
    "dotenv": "^16.1.3",
    "source-map-support": "^0.5.21"
  }
}
